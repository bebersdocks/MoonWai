{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NETCore Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/MoonWai.Api/bin/Debug/net5.0/MoonWai.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/MoonWai.Api",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Elmish Launch",
            "type": "chrome",
            "request": "launch",
            "preLaunchTask": "watchElmish",
            "url": "http://localhost:5000",
            "webRoot": "${workspaceFolder}/MoonWai.Elmish/public",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///*": "${workspaceFolder}/MoonWai.Elmish/src/*",
            }
        },
    ]
}